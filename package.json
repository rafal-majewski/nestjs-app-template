{
	"name": "nestjs-app-template",
	"version": "0.0.0",
	"description": "Nest.js app template",
	"main": "./src/main.ts",
	"scripts": {
		"start": "NODE_ENV=production node ./dist/src/main.js",
		"compile": "tsc",
		"preview": "node ./dist/src/main.js",
		"dev": "ts-node-esm ./src/main.ts",
		"generate-openapi": "ts-node-esm ./src/generate-openapi/main.ts",
		"jest:check": "jest --passWithNoTests --coverage",
		"prettier:fix": "prettier --write .",
		"prettier:check": "prettier --check .",
		"eslint:fix": "eslint --fix .",
		"eslint:check": "eslint --max-warnings 0 .",
		"typescript:check": "tsc --noEmit",
		"npm-audit:check": "npm audit --audit-level=low",
		"npm-audit:fix": "npm audit fix",
		"formatting:fix": "prettier --write .",
		"formatting:check": "prettier --check .",
		"compile:check": "tsc --noEmit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Rafal-Majewski/nestjs-app-template.git"
	},
	"author": "Rafa≈Ç Majewski",
	"license": "CC0-1.0",
	"bugs": {
		"url": "https://github.com/Rafal-Majewski/nestjs-app-template/issues"
	},
	"homepage": "https://github.com/Rafal-Majewski/nestjs-app-template#readme",
	"devDependencies": {
		"@nestjs/swagger": "^6.1.4",
		"@nestjs/testing": "^9.2.0",
		"@types/jest": "^29.2.4",
		"@types/node": "^18.11.17",
		"@typescript-eslint/eslint-plugin": "^5.47.0",
		"@typescript-eslint/parser": "^5.47.0",
		"dotenv": "^16.0.3",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-jest": "^27.1.7",
		"jest": "^29.3.1",
		"prettier": "^2.8.1",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.4",
		"yup": "^0.32.11"
	},
	"dependencies": {
		"@nestjs/common": "^9.1.6",
		"@nestjs/core": "^9.1.6",
		"@nestjs/platform-fastify": "^9.1.6",
		"@nestjs/typeorm": "^9.0.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"nest-typed-config": "^2.4.6",
		"pg": "^8.8.0",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.3.11"
	},
	"type": "module"
}
