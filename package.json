{
	"name": "nestjs-app-template",
	"version": "0.0.0",
	"description": "Nest.js app template",
	"private": true,
	"main": "./src/main.ts",
	"scripts": {
		"test": "jest --passWithNoTests --coverage",
		"start": "node ./dist/src/main.js",
		"compile": "tsc",
		"dev": "ts-node-esm ./src/main.ts",
		"dev:watch": "nodemon --watch ./src --exec ts-node-esm ./src/main.ts",
		"generate-openapi": "ts-node-esm ./scripts/generate_openapi/main.ts",
		"jest:check": "jest --passWithNoTests --coverage",
		"jest:check:unit": "jest --testRegex='.+\\.unit\\.test\\.(?:[mc]?js|ts)$' --passWithNoTests --coverage",
		"jest:check:int": "jest --testRegex='.+.\\int.test\\.(?:[mc]?js|ts)$' --passWithNoTests --coverage",
		"prettier:fix": "prettier --write .",
		"prettier:check": "prettier --check .",
		"eslint:fix": "eslint --fix .",
		"eslint:check": "eslint --max-warnings 0 .",
		"typescript:check": "tsc --noEmit",
		"npm-audit:check": "npm audit --audit-level=low",
		"npm-audit:fix": "npm audit fix",
		"formatting:fix": "prettier --write .",
		"formatting:check": "prettier --check .",
		"compile:check": "tsc --noEmit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Rafal-Majewski/nestjs-app-template.git"
	},
	"author": "Rafa≈Ç Majewski",
	"license": "CC0-1.0",
	"bugs": {
		"url": "https://github.com/Rafal-Majewski/nestjs-app-template/issues"
	},
	"homepage": "https://github.com/Rafal-Majewski/nestjs-app-template#readme",
	"devDependencies": {
		"@nestjs/swagger": "^6.2.1",
		"@nestjs/testing": "^9.2.0",
		"@types/jest": "^29.4.0",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.54.0",
		"@typescript-eslint/parser": "^5.54.0",
		"dotenv": "^16.0.3",
		"envalid": "^7.3.1",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-jest": "^27.2.0",
		"jest": "^29.4.3",
		"nodemon": "^2.0.21",
		"prettier": "^2.8.4",
		"testcontainers": "^9.1.1",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5",
		"zod": "^3.21.4"
	},
	"dependencies": {
		"@nestjs/common": "^9.3.9",
		"@nestjs/core": "^9.1.6",
		"@nestjs/platform-fastify": "^9.3.9",
		"@nestjs/typeorm": "^9.0.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"nest-typed-config": "^2.4.7",
		"pg": "^8.9.0",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.3.11"
	},
	"type": "module",
	"overrides": {
		"nest-typed-config": {
			"class-validator": "^0.14.0"
		}
	}
}
